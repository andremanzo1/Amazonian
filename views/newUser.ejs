    <%-  include("partials/head.ejs")    %>
    <body >
     
      <section class="vh-100 bg-image"
               style="background-image: url('https://www.tasteofeverywhere.com/uploads/1/4/0/9/140932072/20220101-170839_orig.jpg');
                      background-repeat: no-repeat;
                      background-position: center;
                      background-size: cover;">
          <div class="mask d-flex align-items-center h-100 gradient-custom-3">
              <div class="container h-100">
                  <div class="row d-flex justify-content-center align-items-center h-100">
                      <div class="col-12 col-md-9 col-lg-7 col-xl-6">
                          <div class="card" style="border-radius: 15px;">
                              <div class="card-body p-5">
                                  <h2 class="text-uppercase text-center mb-5">Create an account</h2>

                                  <form action="/CreateAccount" method="POST" autocomplete="off">
                                      <div data-mdb-input-init class="form-outline mb-4">
                                          <input type="text" name="UserName" class="form-control form-control-lg" />
                                          <label class="form-label">Username</label>
                                          <span id="error"></span>
                                      </div>

                                      <div data-mdb-input-init class="form-outline mb-4">
                                          <input type="text" name="FirstName" class="form-control form-control-lg" />
                                          <label class="form-label">First Name</label>
                                          <span id="error2"></span>
                                      </div>

                                      <div data-mdb-input-init class="form-outline mb-4">
                                          <input type="text" name="LastName" class="form-control form-control-lg" />
                                          <label class="form-label">Last Name</label>
                                          <span id="error3"></span>
                                      </div>

                                      <div data-mdb-input-init class="form-outline mb-4">
                                          <input type="text" name="Email" id="form3Example3cg" class="form-control form-control-lg" />
                                          <label class="form-label" for="form3Example3cg">Email</label>
                                          <span id="error4"></span>
                                      </div>

                                      <div data-mdb-input-init class="form-outline mb-4">
                                          <input type="password" name="Password" id="form3Example4cg" class="form-control form-control-lg" />
                                          <label class="form-label" for="form3Example4cg">Password</label>
                                          <span id="error5"></span>
                                      </div>

                                      <div data-mdb-input-init class="form-outline mb-4">
                                          <input type="text" name="Phone" id="form3Example4cg" class="form-control form-control-lg" />
                                          <label class="form-label" for="form3Example4cg">Phone number</label>
                                          <span id="error11"></span>
                                      </div>

                                      <button class="btn btn-success btn-block btn-lg gradient-custom-4 text-body">Register</button>

                                      <p class="text-center text-muted mt-5 mb-0">Have already an account? <a href="/Login" class="fw-bold text-body"><u>Login here</u></a></p>
                                  </form>

                              </div>
                          </div>
                      </div>
                  </div>
              </div>
          </div>
      </section>
      
      <script src="/js/createAccount.js"></script>

    </body>
    </html>